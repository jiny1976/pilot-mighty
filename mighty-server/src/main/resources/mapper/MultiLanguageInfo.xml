<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.pilot.mighty.dao.MultiLanguageInfoDao">
 
	<select id="selectMultiLanguageInfo" parameterType="java.util.Map" resultType="java.util.HashMap">
		SELECT LANG_TEXT as langText
		  FROM ADM_MULTI_LANGUAGE
		 WHERE LANG_KEYWORD = #{keyword}
		 AND LANG_CODE = #{code}
	</select>
	
</mapper>
